{
  "name": "react-antd-vite-template",
  "version": "0.0.1",
  "author": "hzyhbk",
  "description": "react antd vite template",
  "scripts": {
    "start": "vite --open --config ./config/vite.config.js",
    "build": "vite build --jsx react --config ./config/vite.config.js"
  },
  "dependencies": {
    "@ant-design/icons": "^4.2.1",
    "@pika/react": "^16.13.1",
    "@pika/react-dom": "^16.13.1",
    "antd": "^4.3.5",
    "moment": "^2.27.0",
    "react-router-dom": "^5.2.0",
    "tui-calendar": "^1.12.13"
  },
  "devDependencies": {
    "hasky": "^3.0.2",
    "less": "^3.11.3",
    "lint-staged": "^10.2.9",
    "prettier": "^2.0.5",
    "vite": "1.0.0-beta.8",
    "vite-plugin-react": "^3.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*{.ts,.js}": [
      "prettier --write",
      "git add"
    ]
  }
}
